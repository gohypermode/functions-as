{
  "name": "hmplugin1",
  "version": "1.0.0",
  "description": "Hypermode Example Plugin",
  "author": "Hypermode, Inc.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build:debug": "asc assembly/index.ts --target debug",
    "build:release": "asc assembly/index.ts --target release",
    "build": "npm run build:debug && npm run build:release",
    "pretty": "prettier --write .",
    "pretty:check": "prettier --check ."
  },
  "dependencies": {
    "hypermode-as": "file:../../src",
    "json-as": "^0.7.2"
  },
  "devDependencies": {
    "@assemblyscript/wasi-shim": "^0.1.0",
    "assemblyscript": "^0.27.23",
    "assemblyscript-prettier": "^3.0.1",
    "prettier": "^3.2.4",
    "visitor-as": "^0.11.4"
  },
  "overrides": {
    "assemblyscript": "$assemblyscript"
  }
}
