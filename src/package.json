    "assemblyscript": "^0.27.24",
    "assemblyscript-prettier": "^3.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "visitor-as": "^0.11.4"
  },
  "overrides": {
    "assemblyscript": "$assemblyscript"
