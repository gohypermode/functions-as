{
  "options": {
    "lib": ["./node_modules/@assemblyscript/wasi-shim/assembly"],
    "use": [
      "ASC_WASI=1",
      "abort=wasi_abort",
      "trace=wasi_trace",
      "seed=wasi_seed",
      "crypto=wasi_crypto",
      "performance=wasi_performance",
      "process=wasi_process",
      "Date=wasi_Date",
      "console=~lib/@hypermode/functions-as/assembly/overrides/console/hypermode_console"
    ],
    "exportStart": "_start",
    "exportRuntime": true
  },
  "targets": {
    "debug": {
      "sourceMap": true,
      "debug": true
    },
    "release": {
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": false,
      "noAssert": false
    }
  }
}
